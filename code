import random
name = ""
length = int(input("How many characters you want in your name? "))
everything = ["a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o", "p",
              "q", "r", "s", "t", "u", "v", "w", "x", "y", "z"]
for i in range(0, length):
    name += random.choice(everything)
print(name)
